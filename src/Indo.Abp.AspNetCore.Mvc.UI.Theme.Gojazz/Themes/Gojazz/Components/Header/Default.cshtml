@using Indo.Abp.AspNetCore.Mvc.UI.Theme.Gojazz.Themes.Gojazz.Components.Toolbar
@using Indo.Abp.AspNetCore.Mvc.UI.Theme.Gojazz.Themes.Gojazz.Components.Brand
<!-- [ Header ] start -->
<header class="navbar pcoded-header navbar-expand-lg navbar-light headerpos-fixed header-lightblue">
    <div class="m-header">
        <a class="mobile-menu" id="mobile-collapse1" href="#!"><span></span></a>
        @(await Component.InvokeAsync<BrandViewComponent>())
    </div>
    <a class="mobile-menu" id="mobile-header" href="#!">
        <i class="feather icon-more-horizontal"></i>
    </a>
    <div class="collapse navbar-collapse">
        <ul class="navbar-nav mr-auto">
            <li><span style="font-weight:bold">@ViewBag.Title</span></li>            
        </ul>
        <ul class="navbar-nav ml-auto">
            @(await Component.InvokeAsync<ToolbarViewComponent>())
        </ul>
    </div>
</header>
<!-- [ Header ] end -->
