<abp-card>
    <abp-card-header>
        <abp-card-title>
            Contact
        </abp-card-title>
    </abp-card-header>
    <abp-card-body>
        <abp-container>
            <abp-row>
                <abp-column>
                    <div id="GridContact"></div>
                </abp-column>
            </abp-row>
        </abp-container>
    </abp-card-body>
</abp-card>
<script id="DialogContact" type="text/x-template">
    <div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input id="customerId" name="customerId" type="text" />
                </div>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input id="name" name="name" type="text" />
                </div>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <textarea id="street" name="street" type="text"></textarea>
                </div>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <div class="e-float-input e-control-wrapper">
                    <input id="city" name="city" type="text" />
                </div>
            </div>
            <div class="form-group col-md-4">
                <div class="e-float-input e-control-wrapper">
                    <input id="state" name="state" type="text" />
                </div>
            </div>
            <div class="form-group col-md-4">
                <div class="e-float-input e-control-wrapper">
                    <input id="zipCode" name="zipCode" type="text" />
                </div>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <div class="e-float-input e-control-wrapper">
                    <input id="phone" name="phone" type="text" />
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="e-float-input e-control-wrapper">
                    <input id="email" name="email" type="text" />
                </div>
            </div>
        </div>
    </div>
</script>